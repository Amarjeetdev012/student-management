<body>
 
    <br>
    <div id="entry">
        <strong>entry</strong>
        <form action="/students/timelogs/start" method="POST">
            <label>student entry: </label>
            <button>Submit</button>
        </form>
    </div>
    <br>
    <div id="exit">
        <strong>exit</strong>
        <form action="/students/timelogs/end" method="POST">
            <label>student exit: </label>
            <button>Submit</button>
        </form>
    </div>
    <br>
  
    <br>
    <div id="students by id">
        <strong>students by id</strong>
        <form action="/students/:id" method="GET">
            <label>students by id : </label>
            <button>Submit</button>
        </form>
    </div>
